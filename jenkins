pipeline{
   agent any
   tools{
     jdk 'jdk21'
   }
  stages{
      stage('checkout'){
        steps{
      git branch:"main",url:"https://github.com/1ms24mc038-Jeevitha/jpjenkin"
      }
  }
  stage('java compile'){
   steps{
      echo'compiling java file'
      sh'javac hellll.java' 
    }
}
 stage(java execution){
   steps{
       echo'executing java program'
       sh 'java hellll.java'
      }
  }
  stage('execute python'){
     steps{
         echo'executing python file'
         sh'python3 hello.py'
       }
   }
}
post{
   success{
       echo'execution successful'
   }
   failure{
        echo'failed to execute'
   }
}
}

